package test;

import java.util.Scanner;

public class random {
	private static final Scanner scan = new Scanner(System.in);

	public static void main(String[] args) {
		
	int a[][]=new int[3][3];
	int n=scan.nextInt();
	for(int i=0;i<=n-1;i++)
	{
		for(int j=0;j<=n-1;j++)
		{
	        a[i][j]=i;
	        
		}
	}
	
	for(int s:a)
	{
      System.out.println(s);
	}
}
}