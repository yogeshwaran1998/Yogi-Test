package test;

import java.util.Scanner;

public class random {
	private static final Scanner scan = new Scanner(System.in);

	public static void main(String[] args) {

	int n=153;
	int r, sum=0;
	int t=0;
	
	r=n%10;
	n=n/10;
	
	while(n>0)
	{
		n=n/10;
	  t=n%10;
	}
		
	sum= (r*r*r) + (n*n*n) + (t*t*t);
}
}