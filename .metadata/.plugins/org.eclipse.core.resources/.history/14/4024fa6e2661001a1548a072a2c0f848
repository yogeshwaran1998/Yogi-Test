package test;

import java.util.Scanner;

public class random {
	private static final Scanner scan = new Scanner(System.in);

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int k=1;
		for (int i = 1; i <= 4; i++)

		{
			for (int j = 1; j <= 4; j++) {

				
				System.out.print(k);
				k++;
				
				if(k>4)
				{
				k=1;
				}
				
                
			}
			System.out.println();

		}

	}
}