package test;

import java.util.Scanner;

public class random {
	private static final Scanner scan = new Scanner(System.in);

	public static void main(String[] args) {

int n=scan.nextInt();

for(int i=2;i<n;i++)
{

			
	if(n%i==0)
	{
		
		System.out.println("Not a prime number");
	break;
	}
	
	else if(i==n-1)
		System.out.println("It is prime number");

}
	
	

	}
}