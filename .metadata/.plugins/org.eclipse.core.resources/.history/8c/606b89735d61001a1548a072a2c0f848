package test;

import java.util.Scanner;

public class random {
	private static final Scanner scan = new Scanner(System.in);

	public static void main(String[] args) {
		
	int a[]=new int[15];
	
	for(int i=0;i<15;i++)
	{
		a[i]=i+1;
		System.out.println(a[i]);
		System.out.println("=======================================");
	}
	
	for(int s:a)
	{
		System.out.println(a[s]);
	}
		

	}
}