package test;

import java.util.Scanner;

public class random {
	private static final Scanner scan = new Scanner(System.in);

	public static void main(String[] args) {

		int n = 153;
		int r = 0;
		int temp = 0;

		while (n > 0) {
			r = n % 10;
			n = n / 10;

			temp = temp + (r * r * r);
		}
	System.out.println(temp);

	}
}