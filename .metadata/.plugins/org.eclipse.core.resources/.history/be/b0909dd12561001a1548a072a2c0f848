package test;

import java.util.Scanner;

public class random {
	private static final Scanner scan = new Scanner(System.in);

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		for (int i = 1; i <= 4; i++)

		{
			for (int j = 1; j <= 4; j++) {

                 int k=i+j-1;
                 
                 if(k>4)
                	 System.out.print(k-4+" ");
                 else
                	 System.out.println(k+" ");
                
			}
			System.out.println();

		}

	}
}